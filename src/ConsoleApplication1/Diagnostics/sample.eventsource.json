{
  "ProviderName": "CE-CodeEffect.Diagnostics.EventSourceGenerator-sample",
  "Settings": {
    "AutogenerateLoggerInterfaces":  false 
  },
  "Keywords": [
    "HostInitialization",
    "Domain",
    "Infrastructure"
  ],
  "TypeTemplates": [
    {
      "Name": "Exception",
      "CLRType": "System.Exception",
      "Arguments": [
        {
          "Assignment": "$this.Message",
          "Name": "message",
          "Type": "string"
        },
        {
          "Assignment": "$this.Source",
          "Name": "source",
          "Type": "string"
        },
        {
          "Assignment": "$this.GetType().FullName",
          "Name": "exceptionTypeName",
          "Type": "string"
        },
        {
          "Assignment": "$this.AsJson()",
          "Name": "exception",
          "Type": "string"
        }
      ]
    }
  ],
  "Loggers": [
    {
      "Name": "IDependencyLogger",
      "StartId": 2000,
      "ImplicitArguments": [
        {
          "Name": "processId",
          "Type": "int"
        },
        {
          "Name": "machineName",
          "Type": "string",
          "Assignment": "Environment.MachineName"
        },
        {
          "Name": "requestName",
          "Type": "string",
          "Assignment": "System.Diagnostics.Process.GetCurrentProcess().Id.ToString()"
        }
      ],
      "OverrideArguments": []
    }
  ],
  "Events": [
  ]
}