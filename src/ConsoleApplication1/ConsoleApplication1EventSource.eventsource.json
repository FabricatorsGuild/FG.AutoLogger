{
  "Loggers": [
    {
      "Name": "IConsoleLogger",
      "StartId": 1000,
      "ImplicitArguments": [
        {
          "Name": "autogenerated",
          "Type": "bool"
        },
        {
          "Name": "machineName",
          "Type": "string",
          "Assignment": "Environment.MachineName"
        }
      ],
      "OverrideArguments": []
    },
    {
      "Name": "IConsoleRunnerLogger",
      "StartId": 2000,
      "ImplicitArguments": [
        {
          "Name": "autogenerated",
          "Type": "bool"
        },
        {
          "Name": "machineName",
          "Type": "string",
          "Assignment": "Environment.MachineName"
        }
      ],
      "OverrideArguments": []
    },
    {
      "Name": "IDependencyLogger",
      "StartId": 3000,
      "ImplicitArguments": [
        {
          "Name": "autogenerated",
          "Type": "bool"
        },
        {
          "Name": "machineName",
          "Type": "string",
          "Assignment": "Environment.MachineName"
        }
      ],
      "OverrideArguments": []
    },
    {
      "Name": "IInfrastructureLogger",
      "StartId": 4000,
      "ImplicitArguments": [
        {
          "Name": "autogenerated",
          "Type": "bool"
        },
        {
          "Name": "machineName",
          "Type": "string",
          "Assignment": "Environment.MachineName"
        }
      ],
      "OverrideArguments": []
    }
  ],
  "Name": "ConsoleApplication1EventSource",
  "ProviderName": "ConsoleApplication1-Default",
  "Keywords": [
    "Console",
    "Error",
    "ConsoleRunner",
    "Dependency",
    "Infrastructure"
  ],
  "Tasks": [
    {
      "Name": "CallExternalComponent",
      "Value": 1
    },
    {
      "Name": "RecieveMessage",
      "Value": 2
    }
  ],
  "TypeTemplates": [
    {
      "Name": "Exception",
      "CLRType": "System.Exception",
      "Arguments": [
        {
          "Name": "message",
          "Type": "string",
          "Assignment": "$this.Message"
        },
        {
          "Name": "source",
          "Type": "string",
          "Assignment": "$this.Source"
        },
        {
          "Name": "exceptionTypeName",
          "Type": "string",
          "Assignment": "$this.GetType().FullName"
        },
        {
          "Name": "exception",
          "Type": "string",
          "Assignment": "$this.AsJson()"
        }
      ]
    }
  ],
  "Settings": {
    "AutogenerateLoggerInterfaces": true
  }
}